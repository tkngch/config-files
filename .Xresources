
Xterm*termName: xterm-256color
URxvt*termName: rxvt-unicode-256color

! URxvt.cursorBlink: 0
URxvt.cursorBlink: 1
URxvt.scrollBar: false
! URxvt.font: xft:DejaVu Sans Mono:style=Book:pixelsize=12, \
URxvt.font: xft:DejaVu Sans Mono:pixelsize=12, \
            xft:WenQuanYi Bitmap Song:size=8, \
            xft:FreeSerif:style=Regular, \
            xft:unifont:style=Medium:antialias=false

! URxvt.perl-ext-common: default,url-select
! URxvt.keysym.M-u: perl:url-select:select_next
! URxvt.keysym.M-Escape: perl:keyboard-select:activate
! URxvt.keysym.M-s: perl:keyboard-select:search

! URxvt.url-select.launcher: /usr/bin/firefox
URxvt.url-select.underline: true

! ibus input
URxvt.inputMethod: ibus
URxvt.predictType: OnTheSpot,None

! disable keycap picture insert mode
URxvt.iso14755: false
URxvt.iso14755_52: false

! fix color issues
URxvt.intensityStyles: false


! color
! baskerville - ivory dark
! taken from http://www.xcolors.net/

*background:  #2D2C28
*foreground:  #A4A6AB

! BLACK
*color0:      #5B5955
*color8:      #707277

! WHITE
*color7:      #DBDDE2
*color15:     #F7F9FF

! RED
*color1:      #C4756E
*color9:      #F6A299

! YELLOW
*color3:      #9B8A4B
*color11:     #CAB775

! GREEN
*color2:      #559A6A
*color10:     #82C896

! CYAN
*color6:      #019BAA
*color14:     #53CAD9

! BLUE
*color4:      #6A8DCA
*color12:     #98BBFB

! MAGENTA
*color5:      #B577AC
*color13:     #E5A4DB
